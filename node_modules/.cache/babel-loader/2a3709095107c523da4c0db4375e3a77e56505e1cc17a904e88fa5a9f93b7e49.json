{"ast":null,"code":"import Reviews from '@/components/product/Reviews.vue';\nimport { mapActions, mapGetters } from 'vuex';\nexport default {\n  name: 'ProductPage',\n  props: ['id'],\n  // Recebe o ID da rota como prop\n  components: {\n    Reviews\n  },\n  data() {\n    return {\n      loading: true // Controle de carregamento\n    };\n  },\n  computed: {\n    ...mapGetters(['Product']),\n    // Mapeia o getter para acessar o estado do produto\n    product() {\n      return this.Product;\n    }\n  },\n  methods: {\n    ...mapActions(['fetchProduct']) // Mapeia a action para buscar o produto\n  },\n  async created() {\n    // Chama a action para buscar o produto com base no ID\n    await this.fetchProduct(this.id);\n    this.loading = false;\n  }\n};","map":{"version":3,"names":["Reviews","mapActions","mapGetters","name","props","components","data","loading","computed","product","Product","methods","created","fetchProduct","id"],"sources":["/Users/vagnerzezo/Documents/development/project/zmoveis/src/components/product/Product.vue"],"sourcesContent":["<template>\n     <div v-if=\"product\" class=\"mx-auto max-w-2xl \">\n          <div class=\"m-40\"></div>\n          <div class=\"mt-8 px-4 pb-16 sm:px-6 sm:pb-24 lg:max-w-7xl lg:px-8\">\n               <div class=\"lg:grid lg:auto-rows-min lg:grid-cols-12 lg:gap-x-8\">\n                    <div class=\"lg:col-span-5 lg:col-start-8\">\n                         <div class=\"flex justify-between\">\n                              <h1 class=\"text-xl font-medium text-gray-900\">{{ product.title }}</h1>\n                              <p class=\"text-xl font-medium text-gray-900\">{{ product.price }}</p>\n                         </div>\n                         <!-- Reviews -->\n                         <div class=\"mt-4 flex justify-between\">\n                              <div>\n                                   <span class=\"text-sm font-semibold\">Marca: </span>\n                                   <span class=\"text-xs\">{{ product.brand }}</span>\n                              </div>\n                              <div>\n                                   <span class=\"text-sm font-semibold\">Sku: </span>\n                                   <span class=\"text-xs\">{{ product.sku }}</span>\n                              </div>\n                         </div>\n                    </div>\n\n                    <!-- Image gallery -->\n                    <div class=\"mt-8 lg:col-span-7 lg:col-start-1 lg:row-span-3 lg:row-start-1 lg:mt-0\">\n                         <h2 class=\"sr-only\">Images</h2>\n\n                         <div class=\"grid grid-cols-1 lg:grid-cols-2 lg:grid-rows-3 lg:gap-8\">\n                              <img v-for=\"image in product.images\" :key=\"image.id\" :src=\"image\" :alt=\"image.imageAlt\"\n                                   :class=\"[image.primary ? 'lg:col-span-2 lg:row-span-2' : 'hidden lg:block', 'rounded-lg']\" />\n                         </div>\n                    </div>\n\n                    <div class=\"mt-8 lg:col-span-5\">\n                         <div class=\"mt-10\">\n                              <div class=\"mt-4 space-y-4 text-sm/6 text-gray-500\" v-html=\"product.description\" />\n                         </div>\n                    </div>\n               </div>\n          </div>\n          <Reviews :reviews=\"product\" />\n     </div>\n</template>\n\n<script>\nimport Reviews from '@/components/product/Reviews.vue'\nimport { mapActions, mapGetters } from 'vuex';\n\nexport default {\n     name: 'ProductPage',\n     props: ['id'], // Recebe o ID da rota como prop\n     components: {\n          Reviews\n     },\n     data() {\n          return {\n               loading: true, // Controle de carregamento\n          };\n     },\n     computed: {\n          ...mapGetters(['Product']), // Mapeia o getter para acessar o estado do produto\n          product() {\n               return this.Product;\n          }\n     },\n     methods: {\n          ...mapActions(['fetchProduct']), // Mapeia a action para buscar o produto\n     },\n     async created() {\n          // Chama a action para buscar o produto com base no ID\n          await this.fetchProduct(this.id);\n          this.loading = false;\n     }\n};\n</script>"],"mappings":"AA6CA,OAAOA,OAAM,MAAO,kCAAiC;AACrD,SAASC,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACVC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE,CAAC,IAAI,CAAC;EAAE;EACfC,UAAU,EAAE;IACPL;EACL,CAAC;EACDM,IAAIA,CAAA,EAAG;IACF,OAAO;MACFC,OAAO,EAAE,IAAI,CAAE;IACpB,CAAC;EACN,CAAC;EACDC,QAAQ,EAAE;IACL,GAAGN,UAAU,CAAC,CAAC,SAAS,CAAC,CAAC;IAAE;IAC5BO,OAAOA,CAAA,EAAG;MACL,OAAO,IAAI,CAACC,OAAO;IACxB;EACL,CAAC;EACDC,OAAO,EAAE;IACJ,GAAGV,UAAU,CAAC,CAAC,cAAc,CAAC,CAAC,CAAE;EACtC,CAAC;EACD,MAAMW,OAAOA,CAAA,EAAG;IACX;IACA,MAAM,IAAI,CAACC,YAAY,CAAC,IAAI,CAACC,EAAE,CAAC;IAChC,IAAI,CAACP,OAAM,GAAI,KAAK;EACzB;AACL,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}